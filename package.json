{
  "name": "react-circuit",
  "version": "0.3.0",
  "description": "React-friendly interface to CSP channels",
  "main": "./lib/react-circuit.js",
  "module": "./lib/react-circuit.mjs",
  "scripts": {
    "test": "cross-env BABEL_ENV=test jest --useStderr",
    "cover": "cross-env BABEL_ENV=test jest --coverage",
    "format": "prettier --no-semi --single-quote --print-width 100 --write \"{src,test}/**/*.js\"",
    "lint": "npm run format && eslint -c ./config/eslint/.eslintrc.json --ignore-path .gitignore src/**",
    "build": "rollup -c && rollup -c --environment DIST",
    "prepublish": "npm run build"
  },
  "repository": "gt3/react-circuit",
  "author": "Ankit Patel <gt3.someday@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gt3/react-circuit/issues"
  },
  "homepage": "https://github.com/gt3/react-circuit#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^17.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.2.2",
    "jest": "^18.1.0",
    "js-csp": "^0.9.3",
    "prettier": "^1.4.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^2.0.0"
  },
  "peerDependencies": {
    "babel-plugin-dev-expression": "^0.2.1",
    "js-csp": "^0.9.3",
    "react": "^15.5.4",
    "prop-types": "^15.5.10"
  },
  "optionalDependencies": {
    "@gt3/dom-listeners": "0.0.1"
  },
  "jest": {
    "testRegex": "(/test/.*\\.test\\.js$)",
    "testEnvironment": "node"
  },
  "dependencies": {
    "invariant": "^2.2.2",
    "react-addons-shallow-compare": "^15.5.2",
    "warning": "^3.0.0"
  },
  "files": [
    "dist",
    "lib"
  ]
}
